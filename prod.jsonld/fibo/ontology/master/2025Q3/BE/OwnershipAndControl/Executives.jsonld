}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/delegatesControlTo",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "indicates a party to which this legal person has delegated some authority or control"
  } ],
