  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/LegallyDelegatedAuthority",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "party empowered with some level of legal control and corresponding responsibility"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
