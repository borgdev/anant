}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/authorizes",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "endorses, enables, empowers, or gives permission to"
  } ],
