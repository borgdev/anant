  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "principal party"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://www.omg.org/spec/Commons/Organizations/OrganizationMember"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/Signatory"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/ResponsibleParty",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
