    "@id" : "_:b29"
  }, {
    "@id" : "_:b30"
  }, {
    "@id" : "_:b31"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/ControlParties/EntityControllingParty"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/Executives/Bylaws",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/synonym" : [ {
    "@value" : "membership agreement"
  }, {
    "@value" : "byelaws"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "written rules for conduct of a corporation, association, partnership or any organization"
  } ],
