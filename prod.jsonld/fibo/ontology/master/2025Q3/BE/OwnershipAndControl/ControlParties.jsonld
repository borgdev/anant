  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/ControlParties/EntityControllingParty",
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@value" : "It is assumed that since control follows from some form of ownership or contractual instrument, that the range of entities which may fulfil this party role is the same as that for entity ownership, namely a logical union of natural persons, legal persons and formal organizations."
  } ],
  "http://www.w3.org/2004/02/skos/core#editorialNote" : [ {
    "@value" : "This type of party is either asserted to be the case by the entity itself or some other party, or is determined through some analysis or calculation based on the available information about controlling interests."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "party that has the authority to control some legal entity"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/ControlParties/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "entity controlling party"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
