  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/ControlParties/DeFactoControllingInterestParty",
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@value" : "divides further into financial leverage via loans; non fiscal types of leverage (influence)"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "a silent partner, i.e. where someone has made a large investment, which is bilateral (not part of the constitutional framework of the company)"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "party that exercises some control over an entity other than via explicit, legal means"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/ControlParties/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "de facto controlling interest party"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b9"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/FND/OwnershipAndControl/Control/ControllingParty"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/ControlParties/DeJureControllingInterestParty",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "party that has the legal authority to exercise control"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/BE/OwnershipAndControl/ControlParties/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "de jure controlling interest party"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b1"
  }, {
