  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "party that establishes a trust and places property under the protection and management of one or more trustees for the benefit of at least one beneficiary"
