  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/LOAN/RealEstateLoans/Mortgages/"
  } ],
  "http://purl.org/dc/terms/source" : [ {
