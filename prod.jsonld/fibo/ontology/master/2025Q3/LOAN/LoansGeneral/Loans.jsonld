}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansGeneral/Loans/LenderLienPosition",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "classifier indicating whether the lender has the primary lien position with respect to an asset used as collateral for the loan"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/LOAN/LoansGeneral/Loans/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "lender lien position"
