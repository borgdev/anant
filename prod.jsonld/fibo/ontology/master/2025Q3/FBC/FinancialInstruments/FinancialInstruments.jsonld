  "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/EquityInstrument",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/adaptedFrom" : [ {
    "@value" : "ISO 10962, Securities and related financial instruments - Classification of Financial Instruments (CFI code), Fourth edition, 2019-10."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "financial instrument representing an ownership interest in an entity or pool of assets"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
