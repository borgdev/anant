  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "commodity instrument"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "The value of the contract is derived from the market value of an underlying commodity or commodity index. Commodity derivatives can be physically or cash settled. Primary underliers include metals, agricultural goods and energy."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "financial instrument representing an ownership interest in bulk goods, such as raw materials and primary agricultural products"
  } ]
