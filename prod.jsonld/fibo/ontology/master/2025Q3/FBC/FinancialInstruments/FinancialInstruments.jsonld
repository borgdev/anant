}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/ExemptSecurity",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "Generally, securities must be filed with the appropriate regulatory agencies in the jurisdiction in which they are sold. The registration forms companies file provide essential facts while minimizing the burden and expense of complying with the law. Not all securities must be registered, however. By exempting many small offerings from the registration process, regulators seek to foster capital formation by lowering the cost of offering securities to the public."
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/adaptedFrom" : [ {
    "@value" : "Securities Act of 1933"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "Some exemptions from the registration requirement include: private offerings to a limited number of persons or institutions; offerings of limited size; intrastate offerings; and securities of municipal, state, and federal governments."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "security that is exempt from certain regulatory rules"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "exempt security"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/Security"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/FinancialInstrument",
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b15"
  }, {
    "@id" : "_:b10"
  }, {
    "@id" : "_:b16"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/WrittenContract"
  }, {
    "@id" : "_:b9"
  }, {
    "@id" : "_:b17"
  }, {
    "@id" : "_:b18"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "A financial instrument can be thought of as a template that defines an arrangement structure that remains to be fleshed out with terms and parameters in order to establish a specific instance of the contract."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "Examples of financial instruments include: cash, evidence of an ownership interest in an entity, or a contractual right to receive (or deliver) cash, or another financial instrument."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "written contract that gives rise to both a financial asset of one entity and a financial liability of another entity"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/adaptedFrom" : [ {
    "@value" : "https://www.ifrs.org/content/dam/ifrs/publications/pdf-standards/english/2021/issued/part-a/ias-32-financial-instruments-presentation.pdf",
    "@type" : "http://www.w3.org/2001/XMLSchema#anyURI"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "financial instrument"
  } ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/FinancialInstrumentIdentifier",
  "http://www.w3.org/2004/02/skos/core#scopeNote" : [ {
    "@value" : "Identifiers for financial instruments may include an ISIN, Sedol, CUSIP, BBGID, FIGI, or other identifier issued approximately when the instrument itself is issued, and based on the kind of instrument and jurisdiction in which it is issued."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "sequence of characters uniquely identifying a financial instrument for some purpose and within a specified context"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/FBC/FinancialInstruments/FinancialInstruments/"
