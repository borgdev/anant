}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/AlignedCommunityInvestmentFund",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "For example, an entrepreneurial support organization (ESO) that derives most of its income from providing consulting services to early stage businesses, can raise capital publicly from its community and deploy some of that capital via investments into its client businesses. These investments would comprise its ACIF.",
    "@language" : "en"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/abbreviation" : [ {
    "@value" : "ACIF",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "community investment fund that raises capital publicly from community members and deploys some of that capital via investments into its client businesses or other businesses in the community",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/"
  } ],
  "http://purl.org/dc/terms/source" : [ {
    "@value" : "Aligned Community Investment Fund, available at https://www.nc3now.org/uploads/1/4/0/1/140134350/acif_overview.pdf.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "aligned community investment fund",
    "@language" : "en"
  } ],
