}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/NonprofitFund",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "A nonprofit fund is a pool of financial resources that is established and managed by a nonprofit organization to support its mission and activities, organized for charitable, educational, religious, cultural, or other purposes recognized as serving the public good. Some nonprofit funds are restricted by donors for a specific use (such as an endowment for scholarships), while others are unrestricted and can be used at the nonprofit's discretion.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "Common examples include endowment funds (permanently invested, only earnings are spent), operating funds (used for day-to-day expenses), and special project funds (earmarked for particular initiatives).",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "investment vehicle designed to support a nonprofit mission, whose objectives include environmental stewardship and/or social responsibility in addition to financial performance",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/SEC/Funds/Funds/"
