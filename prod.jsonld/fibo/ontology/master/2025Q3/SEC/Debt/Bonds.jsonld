  "@id" : "https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/SecuredBond",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "A secured bond may be collateralized by a claim on real assets, such as a factory or auto fleet; or by a claim on a revenue stream. A secured bond differs from a mortgage in that proceeds of the bond sale aren't used to acquire the asset."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "bond that is backed by collateral, such as a tangible asset or income stream, in addition to a general promise to pay"
  } ],
