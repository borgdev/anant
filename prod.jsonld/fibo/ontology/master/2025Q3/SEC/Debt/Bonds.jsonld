  "@id" : "https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/OriginalIssueDiscountBond",
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b58"
  }, {
