  }, {
    "@id" : "_:b5"
  }, {
    "@id" : "https://www.omg.org/spec/Commons/ContextualDesignators/Context"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/FND/Places/Facilities/Venue",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "site where something happens, described in the context of the event or activity that occurs there"
  } ],
