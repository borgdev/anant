}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/CreditDerivatives/CreditDefaultSwaps/ContingentCreditDefaultSwap",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "In a contingent credit default swap, the trigger requires both a credit event (as in a traditional credit default swap) and another specified event. The additional specified event is usually a significant movement in an index covering equities, commodities, interest rates, or some other overall measure of the economy or relevant industry.",
    "@language" : "en"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/abbreviation" : [ {
    "@value" : "CCDS",
    "@language" : "en"
