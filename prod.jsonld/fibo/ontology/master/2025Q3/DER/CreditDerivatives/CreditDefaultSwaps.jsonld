  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/CreditDerivatives/CreditDefaultSwaps/CreditDefaultSwapIndex",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "index whose underlying reference obligations are credit default swap instruments",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/CreditDerivatives/CreditDefaultSwaps/"
