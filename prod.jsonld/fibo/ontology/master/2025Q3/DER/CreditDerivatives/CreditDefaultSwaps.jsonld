  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/CreditDerivatives/CreditDefaultSwaps/CreditEventNotice",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "Notices of certain kinds of credit events are required as a condition of a credit default swap. Such notices are sent from a notifying party (either the buyer or the seller) to the counterparty. They provide information that assists the contract parties in determining whether a triggering credit event has occurred.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "irrevocable written or verbal notice that states that a triggering event has occurred",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/CreditDerivatives/CreditDefaultSwaps/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "credit event notice",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b5"
  }, {
    "@id" : "_:b6"
  }, {
    "@id" : "https://www.omg.org/spec/Commons/Documents/Notice"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
