  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/CurrencyContracts/CurrencyForwardOutright",
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/CurrencyContracts/CurrencyForward"
  }, {
    "@id" : "_:b2"
  }, {
    "@id" : "_:b3"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/synonym" : [ {
    "@value" : "outright forward currency transaction",
    "@language" : "en"
  }, {
    "@value" : "forward outright",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/CurrencyContracts/"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "The holders of a currency forward are obligated to buy or sell the currency at a specified price, at a specified quantity and on a specified future date. These contracts cannot be transferred. Jan 10 Review Notes Outright Forward is the term for the professional markets. Spot + Swap where Swap is 2 simultaneous transactions.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "currency forward outright",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "forward contract in a foreign exchange market that locks in the price at which an entity must buy or sell a currency on a future date",
    "@language" : "en"
  } ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/CurrencyContracts/CurrencyOption",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/synonym" : [ {
    "@value" : "foreign exchange option",
    "@language" : "en"
  }, {
    "@value" : "forex option",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
