  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "iron butterfly",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b6"
  }, {
    "@id" : "_:b60"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/Butterfly"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/IronCondor",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "The iron condor earns the maximum profit when the underlying asset closes between the middle strike prices at expiration. In other words, the goal is to profit from low volatility in the underlying asset.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "condor strategy consisting of two puts (one long and one short) and two calls (one long and one short), and four strike prices, all with the same expiration date",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "iron condor",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b38"
  }, {
    "@id" : "_:b17"
  }, {
    "@id" : "_:b39"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/CondorSpread"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/LongTermEquityAnticipationSecurity",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "By providing opportunities to control and manage risk or even to speculate, LEAPS are virtually identical to regular options. Expiration dates on LEAPs can range from nine months to three years, which is longer than the holding period for a traditional call or put option. Although they are not available on all stocks, LEAPS are available on most widely held issues.",
    "@language" : "en"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/abbreviation" : [ {
    "@value" : "LEAP",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "equity option that allows the holder to buy or sell shares of stock with expiration dates that are longer than one year, and typically up to three years from issue",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "long-term equity anticipation security",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b33"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/EquityOption"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/Moneyness",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "Moneyness describes the intrinsic value of an option in its current state. The term moneyness is most commonly used with put and call options and is an indicator as to the comparative value of the option with respect to its exercise/strike price. Moneyness can be measured with respect to the underlying stock or other asset's current/spot price or its future price.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "classifier for a derivative relating its strike price to the price of its underlying asset",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "moneyness",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b26"
  }, {
    "@id" : "https://www.omg.org/spec/Commons/Classifiers/Classifier"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/OptionHolder",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "In a call, the option holder has the right, but not the obligation, to buy the underlying asset, while, in a put, the option holder has the right to sell the underlying asset. An option holder may sell the option contract itself, at which point the buyer becomes the option holder. Once the instrument is exercised there are typically additional, relevant obligations with regard to settlement.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "party that owns an option",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "option holder",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b42"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/FND/OwnershipAndControl/Ownership/Owner"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/OptionIssuer",
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "issuer granting the rights defined in the option in exchange for some consideration",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/"
