  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/IronButterfly",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/synonym" : [ {
    "@value" : "iron fly",
    "@language" : "en"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "The key to using this trade as part of a successful trading strategy is to forecast a time when option prices are likely to decline in value generally. This usually occurs during periods of sideways movement or a mild upward trend.",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "butterfly strategy that consists of two call options and two put options, three strike prices and the same expiration date",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/Options/"
