}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/ForwardRateAgreement",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "The FRA determines the rates to be used along with the termination date and notional value. FRAs are cash-settled with the payment based on the net difference between the interest rate of the contract and the floating rate in the market called the reference rate. The notional amount is not exchanged, but rather a cash amount based on the rate differentials and the notional value of the contract.",
    "@language" : "en"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/abbreviation" : [ {
    "@value" : "FRA",
    "@language" : "en"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "agreement to exchange an interest rate commitment on a notional amount at a future date",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "forward rate agreement",
    "@language" : "en"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/InterestRateDerivative"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/InterestRateDerivative",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/adaptedFrom" : [ {
    "@value" : "ISO 10962:2019, Securities and related financial instruments - Classification of financial instruments (CFI) code, Fourth edition, 2019-10"
  } ],
  "http://www.w3.org/2004/02/skos/core#example" : [ {
    "@value" : "For example, interest rate derivative strategies are the simultaneous trading of two or more rate contracts in which two counterparties agree to exchange interest rate cash flows on defined dates during an agreed period, based on a specified notional amount, from a fixed rate to a floating rate, floating to fixed, fixed to fixed, or floating to floating."
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "rate-based derivative whose underlier is an interest rate"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "interest rate derivative"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#subClassOf" : [ {
    "@id" : "_:b38"
  }, {
    "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/RateBasedDerivative"
  } ],
  "@type" : [ "http://www.w3.org/2002/07/owl#Class" ]
}, {
  "@id" : "https://spec.edmcouncil.org/fibo/ontology/DER/DerivativesContracts/DerivativesBasics/IntroducingBroker",
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/explanatoryNote" : [ {
    "@value" : "Introducing brokers do not accept money, securities, or property (or extend credit in lieu thereof) to margin, guarantee, or secure any trades or contracts that result or may result."
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/adaptedFrom" : [ {
    "@value" : "http://www.cftc.gov/IndustryOversight/Intermediaries/index.htm",
    "@type" : "http://www.w3.org/2001/XMLSchema#anyURI"
  } ],
  "https://www.omg.org/spec/Commons/AnnotationVocabulary/abbreviation" : [ {
    "@value" : "IB"
  } ],
  "http://www.w3.org/2004/02/skos/core#definition" : [ {
    "@value" : "broker that solicits or accepts orders for derivatives that are traded on or subject to the rules of an exchange"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#isDefinedBy" : [ {
